<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:batch="http://www.springframework.org/schema/batch"
	xsi:schemaLocation="http://www.springframework.org/schema/batch http://www.springframework.org/schema/batch/spring-batch-2.0.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd"
	xmlns:context="http://www.springframework.org/schema/context">

	<import resource="classpath:/META-INF/bootstrap/*.xml" />
	<import resource="classpath:/test-job-context.xml" />
	
	<bean class="org.springframework.batch.test.JobRepositoryTestUtils">
		<property name="dataSource" ref="dataSource" />
		<property name="jobRepository" ref="jobRepository" />
	</bean>

</beans>
